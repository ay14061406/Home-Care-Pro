@{
    ViewBag.Title = "Services";
}

<main>


    <div class="row">
        <section class="col-md-4" aria-labelledby="gettingStartedTitle">
            <h2 id="gettingStartedTitle">Book a Service</h2>
            <p>
                Clicl to book a service
            </p>
            <p><a class="btn btn-outline-dark" href="@Url.Action("Book", "Services")">Book &raquo;</a></p>

        </section>
        <section class="col-md-4" aria-labelledby="librariesTitle">
            <h2 id="librariesTitle">Track a Service</h2>
            <p>Click to track a service</p>
            <p><a class="btn btn-outline-dark" href="@Url.Action("Track", "Services")">Track &raquo;</a></p>
