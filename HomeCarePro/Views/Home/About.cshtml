            <p><a class="btn btn-outline-dark" href="@Url.Action("support", "Services")">Customer Support &raquo;</a></p>
        </section>
        <section class="col-md-4" aria-labelledby="hostingTitle">
            <h2 id="hostingTitle">Customize a Service</h2>
            <p>Click to Customize a Service</p>
            <p><a class="btn btn-outline-dark" href="@Url.Action("Customize", "Services")">Customize &raquo;</a></p>
